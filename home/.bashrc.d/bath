# BATH Aliases
if [ -x "$HOME/.bath.sh" ]; then
  alias wyn='~/.bath.sh -d 1900.readytalk.com -c " "; ssh 1900.readytalk.com'
  alias fort='~/.bath.sh -d forinf2.ecovate.com -c " "; ssh forinf2.ecovate.com'
  alias lax='~/.bath.sh -d laxinf2.ecovate.com -c " "; ssh laxinf2.ecovate.com'
  alias work='~/.bath.sh -d 1900.readytalk.com -c " "; ssh -C -D 9000 -L 9001:fornavi:3389 -L 9002:laxnavi:3389 -L 9003:fornfs5:5901 jkbuster@1900.readytalk.com'
fi
